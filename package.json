{
  "name": "atomaax-theme",
  "displayName": "AtomAx Themes",
  "description": "AtomAx Themes for VS Code",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Themes"
  ],
  "preview": true,
  "publisher": "telianedward",
  "license": "MIT",
  "icon": "AtomAx.png",
  "galleryBanner": {
    "color": "#232323",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telianedward/AtomaxAxTheme"
  },
  "bugs": {
    "url": "https://github.com/telianedward/AtomaxAxTheme/issues"
  },
  "keywords": [
    "theme",
    "light",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Default Light+",
        "uiTheme": "vs",
        "path": "./themes/my-light.json"
      },
      {
        "label": "Default High Contrast Light",
        "uiTheme": "hc-light",
        "path": "./themes/my-light_high_contrast.json"
      },
      {
        "label": "Default Colorblind Light",
        "uiTheme": "vs",
        "path": "./themes/my-light_colorblind.json"
      },
      {
        "label": "Default Light+ Tritanopia",
        "uiTheme": "hc-light",
        "path": "./themes/my-light_tritanopia.json"
      },
      {
        "label": "Default Dark+",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark.json"
      },
      {
        "label": "Default Dark+ Dimmed",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark_dimmed.json"
      },
      {
        "label": "Default High Contrast",
        "uiTheme": "hc-black",
        "path": "./themes/my-dark_high_contrast.json"
      },
      {
        "label": "Default Dark+ Colorblind",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark_colorblind.json"
      },
      {
        "label": "Default Dark+ Tritanopia",
        "uiTheme": "hc-black",
        "path": "./themes/my-dark_tritanopia.json"
      }
    ]
  },
  "devDependencies": {
    "@primer/primitives": "7.9.0",
    "chroma-js": "^2.4.2",
    "color": "^4.2.3",
    "nodemon": "^2.0.20"
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js"
  },
  "dependencies": {
    "latest": "^0.2.0",
    "lodash": "^4.17.21"
  }
}
