{
  "name": "atomaax-theme",
  "displayName": "AtomAx Theme",
  "description": "AtomAx Theme for VS Code",
  "version": "0.2.3",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Themes"
  ],
  "preview": true,
  "publisher": "telianedward",
  "license": "MIT",
  "icon": "AtomAx.png",
  "galleryBanner": {
    "color": "#232323",
    "theme": "dark"
  },
  "homepage": "https://github.com/telianedward/AtomaxAxTheme",
  "repository": {
    "type": "git",
    "url": "https://github.com/telianedward/AtomaxAxTheme"
  },
  "bugs": {
    "url": "https://github.com/telianedward/AtomaxAxTheme/issues"
  },
  "keywords": [
    "theme",
    "light",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "AtomAx Dark+",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark.json"
      },
      {
        "label": "AtomAx Dark+ Dimmed",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark_dimmed.json"
      },
      {
        "label": "AtomAx High Contrast",
        "uiTheme": "hc-black",
        "path": "./themes/my-dark_high_contrast.json"
      },
      {
        "label": "AtomAx Dark+ Colorblind",
        "uiTheme": "vs-dark",
        "path": "./themes/my-dark_colorblind.json"
      },
      {
        "label": "AtomAx Dark+ Tritanopia",
        "uiTheme": "hc-black",
        "path": "./themes/my-dark_tritanopia.json"
      },
      {
        "label": "AtomAx Light+",
        "uiTheme": "vs",
        "path": "./themes/my-light.json"
      },
      {
        "label": "AtomAx High Contrast Light",
        "uiTheme": "hc-light",
        "path": "./themes/my-light_high_contrast.json"
      },
      {
        "label": "AtomAx Colorblind Light",
        "uiTheme": "vs",
        "path": "./themes/my-light_colorblind.json"
      },
      {
        "label": "AtomAx Light+ Tritanopia",
        "uiTheme": "hc-light",
        "path": "./themes/my-light_tritanopia.json"
      }
    ]
  },
  "type": "module",
  "devDependencies": {
    "@primer/primitives": "7.9.0",
    "color": "4.2.3",
    "esbuild": "0.15.8",
    "hsl-to-rgb-for-reals": "1.1.1"

  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js"
  },
  "dependencies": {
    "latest": "^0.2.0",
    "lodash": "^4.17.21"
  }
}
